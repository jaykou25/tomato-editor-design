{
  "name": "@jay.kou/tomato-editor-design",
  "description": "番茄编辑器组件",
  "private": false,
  "version": "0.0.1",
  "license": "ISC",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/tomato-editor-design.umd.js",
  "module": "./dist/tomato-editor-design.es.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tomato-editor.es.js",
      "require": "./dist/tomato-editor.umd.js"
    }
  },
  "scripts": {
    "dev": "cross-env SHOW_ALL_COMPONENT_DEMOS=true vite serve docs",
    "dev2": "vite serve docs",
    "docBuild": "rm -rf docs/dist && vite build docs && serve -s docs/dist",
    "build": "rm -rf dist/* && tsc && vite build",
    "publish": "npm publish --access public",
    "ssr": "rm -rf docs/dist && vite-pages ssr docs && serve docs/dist"
  },
  "dependencies": {
    "@jay.kou/tomato-editor": "^0.1.4",
    "classnames": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react": "^1.3.2",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.15",
    "serve": "^12.0.1",
    "typescript": "^4.7.2",
    "vite": "^2.5.6",
    "vite-pages-theme-doc": "^3.1.4",
    "vite-plugin-mdx": "^3.5.6",
    "vite-plugin-react-pages": "^3.1.8"
  }
}
